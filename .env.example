# PeloJams Environment Variables
# Copy this file to .env and fill in your actual values

# Spotify API Configuration
SPOTIFY_CLIENT_ID=your_spotify_client_id_here
SPOTIFY_CLIENT_SECRET=your_spotify_client_secret_here
SPOTIFY_REDIRECT_URI=http://localhost:5173/auth/spotify/callback

# GetSongBPM API
GETSONGBPM_API_KEY=your_getsongbpm_api_key_here

# YouTube/Google API (for YouTube Music)
YOUTUBE_CLIENT_ID=your_youtube_client_id_here
YOUTUBE_CLIENT_SECRET=your_youtube_client_secret_here
YOUTUBE_REDIRECT_URI=http://localhost:5173/auth/youtube/callback

# Application Settings
NODE_ENV=development
VITE_APP_URL=http://localhost:5173

# Optional: Analytics or other third-party services
# ANALYTICS_ID=your_analytics_id_here
# SENTRY_DSN=your_sentry_dsn_here